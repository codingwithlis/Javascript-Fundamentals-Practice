// function expression catSays
var catSays = function(max) {
  var catMessage = "";
  for (var i = 0; i < max; i++) {
    catMessage += "meow ";
  }
  return catMessage;
};
// function declaration helloCat accepting a callback
function helloCat(shouldWork) {
  return "Hello " + shouldWork(3);
}

// pass in catSays as a callback function
helloCat(catSays);



//first part is the same thing as following

function catSays(max){
    var catMessage = "";
  for (var i = 0; i < max; i++) {
    catMessage += "meow ";
  }
  return catMessage;
};

catSays(8);
